{
  "name": "rtlive",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "export NODE_PORT=3300 && next dev -p $NODE_PORT",
    "build": "rm -rf .next/server && rm -rf .next/static && next build",
    "build_aws": "rm -rf .next/server && rm -rf .next/static && DEPLOY_TARGET=aws next build",
    "start": "next start",
    "export": "next export",
    "export_aws": "DEPLOY_TARGET=aws next export",
    "export_site_images": "node scripts/export_site_images.js --port 31384 --output tmp --start-next"
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@sentry/browser": "^5.27.6",
    "@sentry/webpack-plugin": "^1.14.0",
    "@ygoe/msgpack": "^1.0.2",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "antd": "^4.12.2",
    "aws-sdk": "^2.799.0",
    "axios": "^0.19.2",
    "d3": "^5.16.0",
    "execa": "^4.1.0",
    "next": "^9.5.5",
    "node-cache": "^5.1.2",
    "node-sass": "^4.14.1",
    "puppeteer": "^3.3.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^5.2.1",
    "tmp-promise": "^2.1.1",
    "topojson": "^3.0.2",
    "topojson-client": "^3.1.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.12.0",
    "html-loader": "^1.3.2",
    "husky": "^4.3.0",
    "markdown-loader": "^5.1.0",
    "marked": "^1.2.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^2.0.2",
    "webpack-cli": "^3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
